{
	"presets": [
		"@babel/preset-react",
		["@babel/preset-env", {
			"targets": { 
				"node": "current"
				// "browsers": ["> 1%", "last 2 versions", "not ie <= 8"],
			}
		}]
	],
	"plugins": [
		["@babel/plugin-proposal-class-properties"],
		["@babel/plugin-proposal-object-rest-spread", { "loose": true, "useBuiltIns": true }]
	]
}